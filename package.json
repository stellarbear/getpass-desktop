{
  "name": "getpass",
  "author": "stellarbear",
  "description": "stateless cross-platform password manager (generator)",
  "version": "0.12.1",
  "private": true,
  "scripts": {
    "build": "yarn clear && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clear": "rimraf dist && rimraf dist_electron",
    "dev": "vue-cli-service serve",
    "electron:build": "yarn clear && vue-cli-service electron:build",
    "electron:icon": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:dev": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "jdenticon": "^2.1.1",
    "js-sha256": "^0.9.0",
    "vue": "^2.5.22",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.2",
    "vue-swatches": "^1.0.3",
    "vuetify": "^1.5.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "electron": "^13.6.6",
    "electron-icon-builder": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "yarn lint"
  }
}